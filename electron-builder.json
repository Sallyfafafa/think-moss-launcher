{
  "appId": "com.think.moss.launcher",
  "productName": "ThinkMoss Launcher",
  "directories": {
    "output": "out"
  },
  "files": ["dist/**/*"],
  "extraResources": [".local-browsers/**/*"],
  "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
  "publish": null,
  "mac": {
    "icon": "assets/icons/mac.png",
    "target": ["dmg"]
  },
  "win": {
    "icon": "assets/icons/win.png",
    "target": ["nsis"]
  }
}
